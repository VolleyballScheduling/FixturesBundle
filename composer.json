{
    name: "volleyball/fixtures-bundle",
    type: "symfony-bundle",
    description: Volleyball fixtures bundle. It contains the fixtures for the volleyball system",
    keywords: [
        "scheduling",
        "summercamp"
    ],
    homepage: "http://daviddurost.net/projects/volleyball",
    license: "MIT",
    authors: [
        {
        name: "David Durost",
        homepage: "http://daviddurost.net"
        }
    ],
    require: {
        php: ">=5.5"
    },
    require-dev: {
        fzaninotto/faker: "1.2.*",
        doctrine/doctrine-fixtures-bundle: "2.2.*"
    },
    config: {
        bin-dir: "bin"
    },
    autoload: {
        psr-4: {
            Volleyball\FixturesBundle\: ""
        }
    }
}